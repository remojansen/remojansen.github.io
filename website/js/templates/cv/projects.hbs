<h3>{{unbound projects.title}}</h3>
{{#each projects.license}}
	<h4><i class="{{unbound icon_class}}"></i> {{unbound title}}</h4>
	{{#each project}}
		<p>
			<span class="label label-warning">
                <img class="location" alt="" src="{{unbound country_icon_path}}"/>
                {{unbound year}}
            </span>
			<b>
				<a class="link" href="{{unbound url}}">{{unbound name}}</a>
			</b>
		</p>
		{{#each paragraph}}<p class="paragraph">{{{unbound this}}}</p>{{/each}}
        <p class="skill_container">
            {{unbound skill.title}}
            {{#each skill.item}}<span class="label label-default">{{unbound this}}</span> {{/each}}
        </p>
		{{#if display_break}}<hr />{{/if}}
	{{/each}}
{{/each}}
