<h3>{{unbound skills.title}}</h3>
{{#each skill in skills.skill}}
    <div class="row">
        <div class="col-md-4 tright">{{{unbound skill.name}}}</div>
        <div class="col-md-8">
            <div class="progress">
                <div class="progress-bar progress-bar-{{unbound skill.class}}"
                     role="progressbar" style="width: {{unbound skill.rank}}%">
                </div>
            </div>
        </div>
    </div>
{{/each}}
